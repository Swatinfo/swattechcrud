{
    "name": "swattech/crud",
    "description": "Enterprise-grade Crud generator for Laravel 12 with Vuexy theme integration",
    "type": "library",
    "keywords": [
        "laravel",
        "crud",
        "generator",
        "admin",
        "scaffold",
        "database",
        "vuexy",
        "relationships"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Swat Info System",
            "email": "info@swatinfosystem.com",
            "homepage": "https://swatinfosystem.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "doctrine/dbal": "^3.7",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/console": "^12.0",
        "illuminate/filesystem": "^12.0",
        "illuminate/contracts": "^12.0",
        "barryvdh/laravel-ide-helper": "^3.0",
        "maatwebsite/excel": "^3.1",
        "barryvdh/laravel-dompdf": "^2.0",
        "phpoffice/phpspreadsheet": "^4.0",
        "ext-json": "*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8.0",
        "laravel/pint": "^1.13",
        "phpstan/phpstan": "^1.10",
        "fakerphp/faker": "^1.23",
        "laravel/dusk": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "SwatTech\\Crud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SwatTech\\Crud\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SwatTech\\Crud\\SwatTechCrudServiceProvider"
            ],
            "aliases": {
                "Crud": "SwatTech\\Crud\\Facades\\Crud"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint",
        "analyze": "vendor/bin/phpstan analyse"
    }
}